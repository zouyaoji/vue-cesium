{
  "name": "vue-cesium",
  "private": true,
  "workspaces": [
    "packages/*",
    "website"
  ],
  "description": "Vue 3.x components for CesiumJS.",
  "scripts": {
    "cz": "czg",
    "test": "jest --coverage && coveralls < coverage/lcov.info",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "tsx internal/build/gen-version.ts",
    "update:version": "tsx internal/build/update-version.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C internal/build start",
    "build:theme": "pnpm run -C packages/theme-default build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "stub": "pnpm run -r --parallel stub",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm -y",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\" \"pnpm run -C internal/metadata dev\""
  },
  "author": "zouyaoji <370681295@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zouyaoji/vue-cesium.git"
  },
  "homepage": "https://zouyaoji.top/vue-cesium",
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@turf/circle": "^7.3.0",
    "@vue-cesium/components": "workspace:*",
    "@vue-cesium/composables": "workspace:*",
    "@vue-cesium/directives": "workspace:*",
    "@vue-cesium/locale": "workspace:*",
    "@vue-cesium/shared": "workspace:*",
    "@vue-cesium/theme-default": "workspace:*",
    "@vue-cesium/utils": "workspace:*",
    "@zouyaoji/heatmap.js": "^2.0.8",
    "compare-versions": "^6.1.1",
    "echarts": "^6.0.0",
    "kdbush": "^4.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "mitt": "^3.0.1",
    "proj4": "^2.20.2",
    "urijs": "^1.19.11",
    "vue-cesium": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^1001.0.1",
    "@pnpm/types": "^1001.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.18",
    "@types/gulp-debug": "^2.0.36",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "~24.10.1",
    "@types/three": "^0.181.0",
    "@types/undertaker": "^1.2.12",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitejs/plugin-vue-jsx": "4.2.0",
    "@vue-cesium/build": "workspace:*",
    "@vue/compiler-sfc": "^3.5.24",
    "@vue/test-utils": "2.4.6",
    "algoliasearch": "^5.44.0",
    "chalk": "^5.6.2",
    "commitizen": "^4.3.1",
    "components-helper": "^2.2.0",
    "concurrently": "^9.2.1",
    "consola": "^3.4.2",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "globals": "^16.5.0",
    "gulp": "^5.0.1",
    "gulp-cli": "^3.1.0",
    "gulp-debug": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
